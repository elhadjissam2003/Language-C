#include <stdio.h>
 

int main() {
   int arr[7];
   int n;
    printf("Members : ");
   scanf("%d",&n);
   printf("Input the %d members of the array:\n",n);

    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
         

       
   }
   for(int i=0;i<n;i++){
         if(arr[i]<5){
            printf("arr[%d]=%d",i,arr[i]);
         }

       
   }
  
  
    return 0;
}
