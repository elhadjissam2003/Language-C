 #include <stdio.h>

int main() {
    int val;
    printf("Specific value:");
    scanf("%d",&val);
    printf("\nList of square of each one of the even values from 1 to a %d :\n",val);
    
    for(int i=1;i<=val;i++){
        if(i%2==0){
                    printf("%d ^ 2 = %d \n",i,i*i);
        }

    }
        
    return 0;
}
