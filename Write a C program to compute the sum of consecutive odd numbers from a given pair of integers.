 #include <stdio.h>

int main() {
    int n1,n2;
    int sum=0;
    
    printf("Input first number of the pair: ");
    scanf("%d",&n1);
     printf("\nInput second number of the pair: ");
    scanf("%d",&n2);
    printf("List of odd numbers: \n");
    if(n1>n2){
        n1=n1+n2;
        n2=n1-n2;
        n1=n1-n2;
    }
    for(int i=n1;i<=n2;i++){
        if(i%2!=0){
                    printf("%d\n",i);
                    sum=sum+i;

        }
            
        }
    
    
    printf("\nSum =%d",sum);
    
    
    return 0;
}
