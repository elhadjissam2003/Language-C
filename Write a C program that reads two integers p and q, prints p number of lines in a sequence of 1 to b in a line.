#include <stdio.h>
#include <math.h>


int main() {
    int l,c;
    int compt=1;
    printf("Input number of lines:");
    scanf("%d",&l);
    printf("Number of characters in a line:");
    scanf("%d",&c);
    for(int i=1;i<=l;i++){
      for(int j=1;j<=c;j++){
          printf("%d ",compt);
          compt+=1;
          
      }
            printf("\n");
        }
        
 
    
    
    

    return 0;
}
