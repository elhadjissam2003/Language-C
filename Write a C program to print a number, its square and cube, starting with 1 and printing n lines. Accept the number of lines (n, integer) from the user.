#include <stdio.h>
#include <math.h>


int main() {
    int n;
    int p;
    printf("Input number of lines:");
    scanf("%d",&n);
    for(int i=1;i<=n;i++){
        for(int j=1;j<=3;j++){
            p=pow(i,j);
            
            printf("%d ",p);
            
        }
        
        printf("\n");
    }
    
    
    

    return 0;
}
