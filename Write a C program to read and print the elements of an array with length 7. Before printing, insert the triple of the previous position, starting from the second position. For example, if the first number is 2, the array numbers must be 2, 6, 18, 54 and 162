#include <stdio.h>
 

int main() {
   int arr[7];
   int n;
    printf("Members : ");
   scanf("%d",&n);
   printf("Input the first member of the array:\n",n);
   scanf("%d",&arr[0]);
   printf("n[0]=%d\n",arr[0]);
   
    for(int i=1;i<n;i++){
        arr[i]=arr[i-1]*3;

       
   }
   for(int i=1;i<n;i++){
       printf("n[%d]=%d\n",i,arr[i]);

       
   }
    return 0;
}
