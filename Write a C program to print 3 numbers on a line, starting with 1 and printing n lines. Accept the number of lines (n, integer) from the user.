 #include <stdio.h>

int main() {
    int n;
    int compt=1;
    printf("Input number of lines:");
    scanf("%d",&n);
    for(int i=1;i<=n;i++){
        for(int j=1;j<=3;j++){
            printf("%d ",compt);
            compt+=1;
            
        }
        printf("\n");
    }
 
    return 0;
}
