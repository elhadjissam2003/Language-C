// Online C compiler to run C program online
#include <stdio.h>

int main() {
  int n1,n2;
  int sum=0;
   printf("Input the first integer:\n");
   scanf("%d",&n1);
    printf("Input the second integer:\n");
   scanf("%d",&n2);
   if(n1>n2){
       n1=n1+n2;
       n2=n1-n2;

       n1=n1-n2;
       
   }
   for(int i=n1;i<=n2;i++){
       if(i%17!=0){
           sum+=i;
       
       
   
  
   }
       
   }
   printf("Sum:%d",sum);
  
  
    return 0;
}
