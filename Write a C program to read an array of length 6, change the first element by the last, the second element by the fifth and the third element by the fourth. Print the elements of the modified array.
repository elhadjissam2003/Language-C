#include <stdio.h>
#define AL 6
 

int main() {
   int arr[AL];
   int n;
    printf("Members : ");
   scanf("%d",&n);
   printf("Input the %d members of the array:\n",n);

    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
        
   }
    
    for(int i=0;i<n;i++){
        printf("arr[%d]=%d\n",i,arr[n-i-1]);

       
   }
  
  
    return 0;
}
