// Online C compiler to run C program online
#include <stdio.h>

int main() {
   int pass;
   int i=3;
        
       while(i!=0){

           
       printf("\nInput the password: \n");
       scanf("%d",&pass);
       if(pass == 1234){
           printf("Correct password ");
           i=0;
       }else{
         printf("Incorrect password");
                    
       }
       printf("\n");
       } 
   


   
   
    

    return 0;
}
