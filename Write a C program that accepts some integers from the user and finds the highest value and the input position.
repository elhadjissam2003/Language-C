 #include <stdio.h>

int main() {
    int num,n[20];
    int max ,pos;
   printf("Number of integers :");
   scanf("%d",&num);
   printf("Input %d integers :\n",num);
   for(int i=0;i<num;i++ ){
      
       scanf("%d",&n[i]);
       if(i==0){
            max=n[0];
            
        }else if(max <n[i]){
                max=n[i];
                pos=i+1;
            }
   }
   printf(" Highest value: %d\n",max);
      printf("Position: %d\n",pos);

   


    return 0;
}
